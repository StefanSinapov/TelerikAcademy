<?xml version="1.0" encoding="utf-8" ?>
<CodeSnippets  xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
	<CodeSnippet Format="1.0.0">
		<Header>
			<Title>arrt</Title>
			<Shortcut>arrt</Shortcut>
			<Description>Code snippet for type[] myArray = { ... }</Description>
			<SnippetTypes>
				<SnippetType>Expansion</SnippetType>
			</SnippetTypes>
		</Header>
		<Snippet>
			<Declarations>
				<Literal Editable="false">
					<ID>SystemConsole</ID>
					<Function>SimpleTypeName(global::System.Console)</Function>
				</Literal>
				<Literal>
					<ID>int</ID>
					<Default>int</Default>
				</Literal>
				<Literal>
					<ID>elements</ID>
				</Literal>
				<Literal>
					<ID>myArray</ID>
					<Default>myArray</Default>
				</Literal>
			</Declarations>
			<Code Language="csharp"><![CDATA[$int$[] $myArray$ = { $elements$ };$end$]]>
			</Code>
		</Snippet>
	</CodeSnippet>
</CodeSnippets>